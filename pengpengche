#!/bin/bash
#useræ•°ç»„0-xï¼Œ1-yï¼Œ2-vxï¼Œ3-vyï¼Œ4-axï¼Œ5-ayï¼Œ6-rï¼Œ7-mï¼Œ8-n
user1={}
user2={}
user3={}
user4={}
user1[0]=10000
user1[1]=10000
user2[0]=40000
user2[1]=40000
user1[2]=0
user1[3]=0
user1[4]=0
user1[5]=0
user1[8]=0

user2[2]=0
user2[3]=0
user2[4]=0
user2[5]=0
user2[8]=0
#${user1[*]} ${user2[*]} ${user3[*]} ${user4[*]}
move(){
#userå˜é‡ç”¨æ¥å­˜æ”¾å°è½¦çš„ä½ç½®ã€é€Ÿåº¦ã€åŠ é€Ÿåº¦ã€åŠå¾„ã€è´¨é‡ã€æ˜¯å¦å·²ç»è®¡ç®—ç¢°æ’ž
if [[$user1[8] == 0]]
then
$user1[0]=$user1[0]+$user1[2]
$user1[1]=$user1[1]+$user1[3]
echo "$user1[0] $user1[1]" > userf.dat
fi
if $user2[8] -eq 0
then
$user2[0]=$user2[0]+$user2[2]
$user2[1]=$user2[1]+$user2[3]
echo "$user2[0] $uver2[1]" > users.dat
fi
if $user3[8] -eq 0
then
$user3[0]=$user3[0]+$user3[2]
$user3[1]=$user3[1]+$user3[3]	
fi
if $user4[8] -eq 0
then
$user4[0]=$user4[0]+$user4[2]
$user4[1]=$user4[1]+$user4[3]
fi
#æ›´æ–°å°è½¦ä½ç½®
}
#${user1[*]} ${user2[*]} ${user3[*]} ${user4[*]}
speed(){
$user1[2]=$user1[2]+$user1[4]
$user1[3]=$user1[3]+$user1[5]
$user2[2]=$user2[2]+$user2[4]
$user2[3]=$user2[3]+$user2[5]
$user3[2]=$user3[2]+$user3[4]
$user3[3]=$user3[3]+$user3[5]
$user4[2]=$user4[2]+$user4[4]
$user4[3]=$user4[3]+$user4[5]
#æ›´æ–°å°è½¦é€Ÿåº¦
}

crash(){
if [`echo"scale=2;(($1+$ball1[2])-($3+$ball2[2]))**2+(($2+$ball2[3])-($4+$ball2[3]))**2"|bc`] -lt [`echo"scale=2; ($ball1[6]+$ball2[6])**2"|bc`]
then
k1=`expr $2/$1`
k2=`expr $4/$3`
y=`echo "scale=2;($(k2)*2-$(k1)*4-$k1*$k2*(1-3))/($k2-$k1)"|bc`
x=`echo "scale=2;($y-$2)/$k1+1"|bc`â€
#è®¡ç®—ç¢°æ’žè·¯çº¿äº¤ç‚¹
#until $n -eq 5 || $error -le 0.01
#do
#dot1x=($1+$x)/2
#dot1y=($2+$y)/2
#dot2x=($3+$x)/2
#dot2y=($3+$y])/2
local dip1x=`echo "scale=2;$1-$x"|bc`
local dip1y=`echo "scale=2;$2-$y"|bc`
local dip2x=`echo "scale=2;$3-$x"|bc`
local dip2y=`echo "scale=2;$4-$x"|bc`
#ç›®å‰åæ ‡äºŽäº¤ç‚¹çš„è·ç¦»
local i=1
local j=1
local k=1
error[25]
for i in 1 2 3 4 5
do
for j in 1 2 3 4 5
do
error[($j-1)+5*($i-1)]=` echo "scale=2;ï¼ˆ$x+$i*$dip1x-($x+$j*$ dip2x)ï¼‰**2+($y+$i*$dip1y-($y+$j*$ dip2y))**2-$ball1[6]**2-$ball2[6]**2"|bc`
$j=$j+1
done
$i=$i+1
$j=0
done
while $k -le 24
do
if $error[$k] -lt 0
then $error[$k]=-$error[$k]
fi
done
$i=1
$j=0
while $i -le 24
do
$serror=$error[0]
if $vserror -lt $error[i]
then $serror=$error[i]
$j=$i
fi
$i=$i+1
done
#æžšä¸¾é€¼è¿‘ç¢°æ’žç‚¹
$i=expr`($j+1)/5`
$j=expr`($j+1)%5`
p1x=expr`$x+$i*$dip1x`
p1y=expr`$y+$i*$dip1y`
p2x=expr`$x+$j*$dip2x`
p2y=expr`$y+$j*$dip2y`
#è¿™æ˜¯æ‹Ÿåˆå‡ºç¢°æ’žæ—¶ä¸¤ä¸ªçƒçƒå¿ƒçš„ä½ç½®
local t=($p1x-$1)/ball1[2]
#è®¡ç®—ç¢°æ’žå‰è¿åŠ¨æ—¶é—´
$k=`echo "scale=2;($p1y-$p2y)/($p1x-$p2x) "|bc`
$k_=-1/$k
#è®¡ç®—ç¢°æ’žå¾„å‘ä¸Žè½´å‘
sinx=`echo "scale=2;(1/sqrt((1/$k_)**2+1))"|bc`
cosx=`echo "scale=2;(1/sqrt(($k_)**2+1))"|bc`
#è®¡ç®—æ­£å¼¦å’Œä½™å¼¦
local v1=`echo "scale=2;$1*$sinx+$2*$cosx)"|bc`
local v2=`echo "scale=2;$3*$sinx+$4*$cosx)"|bc`
#æ­¤å¤„æš‚ä»¥äº¤æ¢é€Ÿåº¦ä¸ºç¢°æ’ž
local v1x=$v1*$sinx
local v2x=$v2*$sinx
local v1y=$v1*$cosx
local v2y=$v2*$cosx
$ball1[2]=$ball1[2]-v1x+v2x
$ball1[3]=$ball1[3]-v1y+v2y
$ball2[2]=$ball2[2]+v1x-v2x
$ball2[3]=$ball2[3]+v1y-v2y
#æ›´æ–°é€Ÿåº¦
$t=1-$t
$1= $p1x+$t*$ball1[2]
$2= $p1y+$t*$ball1[3]
$3= $p2x+$t*$ball2[2]
$4= $p2y+$t*$ball2[3]
#æ›´æ–°ä½ç½®
ball1[8]=1
ball2[8]=1
#è¡¨æ˜Žå·²ç»ç¢°æ’ž
#`echo "scale=2;sqrt(2)"|bc`
fi
}


crash_wall(){
local t=1
#ç¢°æ’žæ—¶é—´
if $1+$3+5 -gt $x_range
then 
$t=expr`($xrange_1)/$3`
$t=expr`1-$t`
$3=expr`0 - $3`
$1=expr`x_range-$t*$3`
$6=$1

elif $1+$3+5 -lt -$x_range
then
$t=expr`($1-$xrange)/$3`
$t=expr`1-$t`
$3=expr`0 - $3`
$1=expr`-x_range+$t*$3`
$6=1

elif $2+$4+5 -gt $y_range
then
$t=expr`($x-range_2)/$4`
$t=expr`1-$t`
$4=expr`0 - $4`
$2=expr`y_range-$t*$4`
$6=1

elif $2+$4+5 -lt -$y_range
then
$t=expr`($2-$x_range)/$4`
$t=expr`1-$t`
$4=expr`0 - $4`
$2=expr`-y_range+$t*4`
$6=1

fi
#è¾¹ç•Œç¢°æ’žå‡½æ•°
}



up1()
{
user1[5]= `echo "scale=8; $user1[5] + 0.2"|bc `
}
down1()
{
user1[5]= `echo "scale=8; $user1[5] - 0.2"|bc `
}
